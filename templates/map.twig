{% set people = [{first: "Bob", last: "Smith"},{first: "Alice", last: "Dupond"}] %}
{{ people|map(p => "#{p.first} #{p.last}")|join(', ') }}
{# outputs Bob Smith, Alice Dupond #}

<br>
{% set people = {"Bob": "Smith","Alice": "Dupond"} %}
{{ people|map((last, first) => "#{first} #{last}")|join(', ') }}
{# outputs Bob Smith, Alice Dupond #}
